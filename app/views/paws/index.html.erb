<p id="notice"><%= notice %></p>

<h1>Paws</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @paws.each do |paw| %>
      <tr>
        <td><%= link_to 'Show', paw %></td>
        <td><%= link_to 'Edit', edit_paw_path(paw) %></td>
        <td><%= link_to 'Destroy', paw, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


